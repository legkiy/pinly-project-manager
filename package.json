    "@fontsource/roboto": "^5.1.0",
    "@hookform/resolvers": "^3.10.0",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@tauri-apps/api": "^2.6.0",
    "@tauri-apps/plugin-opener": "^2.4.0",
    "i18next": "^24.2.0",
    "i18next-browser-languagedetector": "^8.0.2",
